{
	"css.enable": true,
	"html.enable": true,
	"json.enable": true,
	"css.validate": true,
	"tsserver.log": "verbose",
	"tsserver.enable": true,
	"tsserver.formatOnType": false,
	"tslint.exclude": ["node_modules"],
	"tslint.jsEnable": true,
	"tslint.autoFixOnSave": true,
	"tslint.ignoreDefinitionFiles": true,
	"tslint.suppressWhileTypeErrorsPresent": false,
	"tslint.alwaysShowRuleFailuresAsWarnings": true,
	"prettier.eslintIntegration": true,
	"prettier.tslintIntegration": true,
	"prettier.stylelintIntegration": true,
	"coc.preferences.bracketEnterImprove": true,
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.formatOnSaveFiletypes": ["typescript", "css", "javascript", "tsx", "jsx", "json", "html", "xml"],
	"coc.preferences.snippets.enable": true,
	"coc.preferences.previewAutoClose": true
}
